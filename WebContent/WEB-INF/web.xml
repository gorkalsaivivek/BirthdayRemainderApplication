<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>BirthdayReminderApplication</display-name>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
    <servlet-name>loginPageServlet</servlet-name>
    <servlet-class>com.prudential.LoginPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginPageServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.prudential.LoginValidate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/validate</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>InsertServlet</servlet-name>
    <servlet-class>com.prudential.InsertUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertServlet</servlet-name>
    <url-pattern>/saveRecord</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>UpdateServlet</servlet-name>
    <servlet-class>com.prudential.UpdateUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateServlet</servlet-name>
    <url-pattern>/updateRecord</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>DeleteServlet</servlet-name>
    <servlet-class>com.prudential.DeleteUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteServlet</servlet-name>
    <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  
  
  
</web-app>